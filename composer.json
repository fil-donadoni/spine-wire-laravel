{
    "name": "fil-donadoni/spine-wire-laravel",
    "description": "Deployment files for Laravel applications on GCP Cloud Run. Companion to spine-core.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "google/apiclient": "^2.18",
        "google/cloud-storage": "^1.30",
        "league/flysystem-google-cloud-storage": "^3.0",
        "google/cloud-pubsub": "^2.0",
        "laravel/octane": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "FilDonadoni\\SpineWireLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilDonadoni\\SpineWireLaravel\\DevOpsServiceProvider",
                "FilDonadoni\\SpineWireLaravel\\Storage\\GoogleCloudStorageServiceProvider",
                "FilDonadoni\\SpineWireLaravel\\PubSub\\GoogleCloudPubSubServiceProvider",
                "FilDonadoni\\SpineWireLaravel\\CloudRun\\CloudRunServiceProvider"
            ]
        }
    }
}
